#---Generate Main Documents---

DOCMAIN := README.md LICENSE

README.md: template/README.md.got
	pgot -i ":$(RTEMPLATE)" -o $@ $<

LICENSE: $(RTEMPLATE)/LICENSE.src/BSD-2-clause.got
	pgot -i ":$(RTEMPLATE)" -o $@ $<

docMain: $(DOCMAIN)

cleanDocMain:
	$(RM) $(DOCMAIN)

.PHONY: docMain, cleanDocMain
{{- /* vim:set noet tw=80: */ -}}
